#include <iostream>

using namespace std;

int main()
{
    int n, m, x, y, k = 0;
    
    cin >> n >> m;
    
    int a[n][m];
    
    x = 0;
    y = 0;
    
    int max_x = n;
    int max_y = m;
    int min_x = 0;
    int min_y = 0;
    int nm = n * m;
    int num = 1;
    
    while (k < nm) 
    {   
        
        for (; y < max_y && k < nm; ++y, ++k)
            a[x][y] = num++;

        --y;
        ++x;
        --max_y;

        for (; x < max_x && k < nm; ++x, ++k)
            a[x][y] = num++;

        --x;
        --y;
        ++min_x;

        for (; y >= min_y && k < nm; --y, ++k)
            a[x][y] = num++;

        ++y;
        --x;
        ++min_y;

        for (; x >= min_x && k < nm; --x, ++k)
            a[x][y] = num++;

        ++x;
        ++y;
        --max_x;

}
   
    for (int i = 1; i <= n; i++) 
    {
        for (int j = 1; j <= m; j++) 
        {
            cout.width(4); 
            cout << a[i-1][j-1];
        }
        cout << endl;
    }
   
  return 0;
}
